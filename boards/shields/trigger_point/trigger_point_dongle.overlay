#include <dt-bindings/zmk/matrix_transform.h>

/ {
    chosen {
        zmk,split-role = "central";
        zmk,matrix-transform = &mt;
    };

    mt: matrix_transform {
        compatible = "zmk,matrix-transform";
        rows = <5>;
        columns = <14>;
        map = <ZMK_MATRIX_TRANSFORM_DIRECT_MAP(5, 14)>;
    };
};